{
  
    "source": {
      "include": ["apps"],
      "exclude": ["apps/client/node_modules","apps/server/node_modules"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "./my_plugin.js"
    
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./docs/JSDocs",
      "readme":"./docs/README.md"
    }
  
}