{
    "source": {
      "include": ["apps"],
      "exclude": ["apps/client/node_modules","apps/server/node_modules"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs/JSDocs",
      "readme":"./docs/README.md",
      "template": "node_modules/better-docs"
    },
    "templates": {
      "better-docs": {
        "name": "Wellplates",
        "navLinks": [
          {
            "label": "ZZZZZ",
            "href": "zzzzz.html"
          }
        ]
      }
    },
    "plugins":[
      "node_modules/better-docs/category"
    ]
  }
  