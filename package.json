{
  "name": "wellplates",
  "version": "0.1.0",
  "homepage": "https://wellplates.selimharfouche.com/",
  "private": true,
  "dependencies": {
    "@react-three/drei": "^9.107.0",
    "@react-three/fiber": "^8.16.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "three": "^0.165.0",
    "web-vitals": "^4.1.1"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "predeploy": "npm run build",
    "build": "react-scripts --openssl-legacy-provider build",
    "add-domain": "echo \"wellplates.selimharfouche.com\" > build/CNAME",
    "deploy": "npm run add-domain && gh-pages -d build",
    "bd": "npm run build && npm run deploy",
    "test": "react-scripts test --openssl-legacy-provider test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-scripts": "^5.0.1"
  }
}
